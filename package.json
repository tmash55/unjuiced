{
  "name": "unjuiced-monorepo",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "dev:web": "npm run dev --workspace=@unjuiced/web",
    "dev:web:lan": "npm run dev:lan --workspace=@unjuiced/web",
    "dev:mobile": "npm run dev --workspace=@unjuiced/mobile",
    "dev:mobile:lan": "npm run dev:lan --workspace=@unjuiced/mobile"
  },
  "devDependencies": {
    "turbo": "^2.3.3"
  }
}
